{
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "diagnostics": true,
    "noEmit": false,
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "newLine": "LF",
    "useDefineForClassFields": false,
    "allowJs": false,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "removeComments": false,
    "baseUrl": "./"
    // "lib": [
    //   "ESNext",
    //   "DOM",
    //   "DOM.Iterable",
    //   "ScriptHost",
    // ]
  },
  "paths": {},
  "ts-node": {
    "esm": true,
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {}
  }
}
